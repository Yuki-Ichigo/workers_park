


<div>
	<h2><strong><%= "#{@company.name}のメンバー" %></strong></h2>
	<br>
	<div class="company-member-flex row col-sm-12">
		<% @members.each do |member| %>
			<div class="col-sm-2">
				<%= link_to profile_path(member.profile) do %>
					<%= attachment_image_tag member, :image, :fill, 100, 100, fallback: "no_image_human.jpeg", size:'100x100', class:"img-circle" %> <br />
					<p><%= member.nic_name %></p>
				<% end %>
			</div>
		<% end %>
	</div>
</div>

<br><br>